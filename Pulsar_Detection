from PFDFile import *
from a170113profile import getFileName
path_neg = "/home/luzihao/xiaoluo/dataset/bin/p309n_pfd"
path_pos = "/home/luzihao/xiaoluo/dataset/bin/p309p_pfd"
flist_neg = getFileName(path_neg)
flist_pos = getFileName(path_pos)
file_num_neg = len(flist_neg)
file_num_pos = len(flist_pos)
pos_array = [0 for i in range(54)]
neg_array = [0 for j in range(54)]

for j in range(file_num_pos):
    file_situ = path_pos+"/"+flist_pos[j]
    cand = PFD(file_situ)
    nums = cand.get_subints()
    cols = shape(nums)[0]
    rols = shape(nums)[1]
    if cols != 64 and rols != 64:
        print(shape(nums))



